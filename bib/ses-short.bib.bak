	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/test.12264},
	keywords = {data literacy, data science, learnr, modeling, Shiny apps, simulation-based inference, teaching, teaching statistics},
	file = {Gehrke et al_(2021)Statistics education from a data-centric perspective.pdf:/Users/sebastiansaueruser/Literatur/Gehrke et al_(2021)Statistics education from a data-centric perspective.pdf:application/pdf;Snapshot:/home/sebastian/snap/zotero-snap/common/Zotero/storage/Y3HNYSCC/test.html:text/html},
