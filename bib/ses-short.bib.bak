	file = {Djikic et al_(2008)Reducing Stereotyping Through Mindfulness - Effects on Automatic Stereotype-Activated Behaviors.pdf:/Users/sebastiansaueruser/Literatur/Djikic et al_(2008)Reducing Stereotyping Through Mindfulness - Effects on Automatic Stereotype-Activated Behaviors.pdf:application/pdf},
