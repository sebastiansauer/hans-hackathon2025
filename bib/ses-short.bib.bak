@article{uttl_meta-analysis_2017,
	title = {Meta-analysis of faculty's teaching effectiveness: Student evaluation of teaching ratings and student learning are not related},
	volume = {54},
	issn = {0191-491X},
	url = {https://www.sciencedirect.com/science/article/pii/S0191491X16300323},
	doi = {10.1016/j.stueduc.2016.08.007},
	series = {Evaluation of teaching: Challenges and promises},
	shorttitle = {Meta-analysis of faculty's teaching effectiveness},
	abstract = {Student evaluation of teaching ({SET}) ratings are used to evaluate faculty's teaching effectiveness based on a widespread belief that students learn more from highly rated professors. The key evidence cited in support of this belief are meta-analyses of multisection studies showing small-to-moderate correlations between {SET} ratings and student achievement (e.g., Cohen, 1980, Cohen, 1981; Feldman, 1989). We re-analyzed previously published meta-analyses of the multisection studies and found that their findings were an artifact of small sample sized studies and publication bias. Whereas the small sample sized studies showed large and moderate correlation, the large sample sized studies showed no or only minimal correlation between {SET} ratings and learning. Our up-to-date meta-analysis of all multisection studies revealed no significant correlations between the {SET} ratings and learning. These findings suggest that institutions focused on student learning and career success may want to abandon {SET} ratings as a measure of faculty's teaching effectiveness.},
	pages = {22--42},
	journaltitle = {Studies in Educational Evaluation},
	shortjournal = {Studies in Educational Evaluation},
	author = {Uttl, Bob and White, Carmela A. and Gonzalez, Daniela Wong},
	urldate = {2023-06-21},
	date = {2017-09-01},
	langid = {english},
	keywords = {Evaluation of faculty, Meta-analysis of student evaluation of teaching, Multisection studies, {SET} and learning correlations, Teaching effectiveness, Validity},
	file = {ScienceDirect Snapshot:/home/sebastian/snap/zotero-snap/common/Zotero/storage/ZC7FGQTS/S0191491X16300323.html:text/html},
}

@article{garland_mindfulness_2015,
	title = {Mindfulness Broadens Awareness and Builds Eudaimonic Meaning: A Process Model of Mindful Positive Emotion Regulation},
	volume = {26},
	issn = {1047-840X},
	url = {https://doi.org/10.1080/1047840X.2015.1064294},
	doi = {10.1080/1047840X.2015.1064294},
	shorttitle = {Mindfulness Broadens Awareness and Builds Eudaimonic Meaning},
	abstract = {Contemporary scholarship on mindfulness casts it as a form of purely nonevaluative engagement with experience. Yet, traditionally mindfulness was not intended to operate in a vacuum of dispassionate observation, but was seen as facilitative of eudaimonic mental states. In spite of this historical context, modern psychological research has neglected to ask the question of how the practice of mindfulness affects downstream emotion regulatory processes to impact the sense of meaning in life. To fill this lacuna, here we describe the mindfulness-to-meaning theory, from which we derive a novel process model of mindful positive emotion regulation informed by affective science, in which mindfulness is proposed to introduce flexibility in the generation of cognitive appraisals by enhancing interoceptive attention, thereby expanding the scope of cognition to facilitate reappraisal of adversity and savoring of positive experience. This process is proposed to culminate in a deepened capacity for meaning-making and greater engagement with life.},
	pages = {293--314},
	number = {4},
	journaltitle = {Psychological Inquiry},
	author = {Garland, Eric L. and Farb, Norman A. and R. Goldin, Philippe and Fredrickson, Barbara L.},
	urldate = {2023-06-23},
	date = {2015-10-02},
	pmid = {27087765},
	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/1047840X.2015.1064294},
	keywords = {mindfulness, affective science, broaden-and-build, emotion regulation, eudaimonic well-being, interoception, mindfulness-to-meaning theory, positive emotion, posttraumatic growth, reappraisal, upward spiral},
	file = {Garland et al_(2015)Mindfulness Broadens Awareness and Builds Eudaimonic Meaning - A Process Model of Mindful Positive Emotion Regulation.pdf:/Users/sebastiansaueruser/Literatur/Garland et al_(2015)Mindfulness Broadens Awareness and Builds Eudaimonic Meaning - A Process Model of Mindful Positive Emotion Regulation.pdf:application/pdf},
}

@incollection{follette_mindfulness_2016,
	location = {Washington, {DC}, {US}},
	title = {Mindfulness and acceptance theories},
	isbn = {978-1-4338-2132-5},
	series = {{APA} handbooks in psychology®},
	abstract = {Mindfulness and acceptance have been associated with the so-called third-wave behavior therapies within clinical psychology. They came from a range of philosophical origins, including problems with mechanistic approaches that focused on eliminating problematic thoughts and feelings. This concern led to a philosophical shift in cognitive–behavioral therapy ({CBT}) associated with a pragmatic analysis of treatment with a emphasis on understanding behaviors as acts in context. Theories associated with this new perspective include acceptance and commitment therapy ({ACT}), dialectical behavior therapy ({DBT}), mindfulness-based cognitive therapy ({MBCT}; an adaptation of mindfulness-based stress reduction, or {MBSR}), and functional analytic psychotherapy. In this chapter, we examine the historical evolution, research evidence, treatment processes, and future directions of mindfulness and acceptance. Although these concepts have most recently been associated with the third-wave therapies, their origins are indeed quite old, with roots in ancient Eastern philosophical and spiritual traditions. A number of theories, such as those from existential–humanistic approaches, have focused on being present and acceptance of one’s current condition. We provide historical and conceptual issues that are shared in the two traditions. We then focus on specifics of some of the dominant therapies that use mindfulness and acceptance in treatment. In concluding, we describe how the integration of mindfulness and acceptance has led to a different approach to modern psychotherapy and offer future directions for the field. ({PsycInfo} Database Record (c) 2020 {APA}, all rights reserved)},
	pages = {273--302},
	booktitle = {{APA} handbook of clinical psychology: Theory and research, Vol. 2},
	publisher = {American Psychological Association},
	author = {Follette, Victoria M. and Hazlett-Stevens, Holly},
	date = {2016},
	doi = {10.1037/14773-010},
	keywords = {Mindfulness, Acceptance and Commitment Therapy, Clinical Psychology, Cognitive Therapy, Dialectical Behavior Therapy, Functional Analysis},
	file = {Snapshot:/home/sebastian/snap/zotero-snap/common/Zotero/storage/Q9MCJ377/2015-25841-010.html:text/html},
}

@article{schuman-olivier_mindfulness_2020,
	title = {Mindfulness and Behavior Change},
	volume = {28},
	issn = {1067-3229},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC7647439/},
	doi = {10.1097/HRP.0000000000000277},
	abstract = {Initiating and maintaining behavior change is key to the prevention and treatment of most preventable chronic medical and psychiatric illnesses. The cultivation of mindfulness, involving acceptance and nonjudgment of present-moment experience, often results in transformative health behavior change. Neural systems involved in motivation and learning have an important role to play. A theoretical model of mindfulness that integrates these mechanisms with the cognitive, emotional, and self-related processes commonly described, while applying an integrated model to health behavior change, is needed. This integrative review (1) defines mindfulness and describes the mindfulness-based intervention movement, (2) synthesizes the neuroscience of mindfulness and integrates motivation and learning mechanisms within a mindful self-regulation model for understanding the complex effects of mindfulness on behavior change, and (3) synthesizes current clinical research evaluating the effects of mindfulness-based interventions targeting health behaviors relevant to psychiatric care. The review provides insight into the limitations of current research and proposes potential mechanisms to be tested in future research and targeted in clinical practice to enhance the impact of mindfulness on behavior change.},
	pages = {371--394},
	number = {6},
	journaltitle = {Harvard Review of Psychiatry},
	shortjournal = {Harv Rev Psychiatry},
	author = {Schuman-Olivier, Zev and Trombka, Marcelo and Lovas, David A. and Brewer, Judson A. and Vago, David R. and Gawande, Richa and Dunne, Julie P. and Lazar, Sara W. and Loucks, Eric B. and Fulwiler, Carl},
	urldate = {2023-06-23},
	date = {2020},
	pmid = {33156156},
	pmcid = {PMC7647439},
	file = {Schuman-Olivier et al_(2020)Mindfulness and Behavior Change.pdf:/Users/sebastiansaueruser/Literatur/Schuman-Olivier et al_(2020)Mindfulness and Behavior Change.pdf:application/pdf},
}

@article{manjaly_computational_2020,
	title = {A Computational Theory of Mindfulness Based Cognitive Therapy from the “Bayesian Brain” Perspective},
	volume = {11},
	issn = {1664-0640},
	url = {https://www.frontiersin.org/articles/10.3389/fpsyt.2020.00404},
	abstract = {Mindfulness Based Cognitive Therapy ({MBCT}) was developed to combine methods from cognitive behavioral therapy and meditative techniques, with the specific goal of preventing relapse in recurrent depression. While supported by empirical evidence from multiple clinical trials, the cognitive mechanisms behind the effectiveness of {MBCT} are not well understood in computational (information processing) or biological terms.This article introduces a testable theory about the computational mechanisms behind {MBCT} that is grounded in “Bayesian brain” concepts of perception from cognitive neuroscience, such as predictive coding. These concepts regard the brain as embodying a model of its environment (including the external world and the body) which predicts future sensory inputs and is updated by prediction errors, depending on how precise these error signals are.This article offers a concrete proposal how core concepts of {MBCT}—(i) the being mode (accepting whatever sensations arise, without judging or changing them), (ii) decentering (experiencing thoughts and percepts simply as events in the mind that arise and pass), and (iii) cognitive reactivity (changes in mood reactivate negative beliefs)—could be understood in terms of perceptual and metacognitive processes that draw on specific computational mechanisms of the “Bayesian brain.” Importantly, the proposed theory can be tested experimentally, using a combination of behavioral paradigms, computational modelling, and neuroimaging. The novel theoretical perspective on {MBCT} described in this paper may offer opportunities for finessing the conceptual and practical aspects of {MBCT}.},
	journaltitle = {Frontiers in Psychiatry},
	author = {Manjaly, Zina-Mary and Iglesias, Sandra},
	urldate = {2023-06-23},
	date = {2020},
	file = {Manjaly&Iglesias_(2020)A Computational Theory of Mindfulness Based Cognitive Therapy from the “Bayesian Brain” Perspective.pdf:/Users/sebastiansaueruser/Literatur/Manjaly&Iglesias_(2020)A Computational Theory of Mindfulness Based Cognitive Therapy from the “Bayesian Brain” Perspective.pdf:application/pdf},
}

@incollection{dunning_dunningkruger_2011,
	title = {The Dunning–Kruger Effect: On Being Ignorant of One's Own Ignorance},
	volume = {44},
	url = {https://www.sciencedirect.com/science/article/pii/B9780123855220000056},
	shorttitle = {Chapter five - The Dunning–Kruger Effect},
	abstract = {In this chapter, I provide argument and evidence that the scope of people's ignorance is often invisible to them. This meta-ignorance (or ignorance of ignorance) arises because lack of expertise and knowledge often hides in the realm of the “unknown unknowns” or is disguised by erroneous beliefs and background knowledge that only appear to be sufficient to conclude a right answer. As empirical evidence of meta-ignorance, I describe the Dunning–Kruger effect, in which poor performers in many social and intellectual domains seem largely unaware of just how deficient their expertise is. Their deficits leave them with a double burden—not only does their incomplete and misguided knowledge lead them to make mistakes but those exact same deficits also prevent them from recognizing when they are making mistakes and other people choosing more wisely. I discuss theoretical controversies over the interpretation of this effect and describe how the self-evaluation errors of poor and top performers differ. I also address a vexing question: If self-perceptions of competence so often vary from the truth, what cues are people using to determine whether their conclusions are sound or faulty?},
	pages = {247--296},
