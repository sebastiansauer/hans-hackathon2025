Publisher: Educational Publishing Foundation},
	keywords = {Stress, Mental Health, Hate, Acculturation, Ethnic Identity, Exposure, Foreign Language Translation, Immigration, Major Depression, Oral Communication, Posttraumatic Stress Disorder},
	file = {Snapshot:/home/sebastian/snap/zotero-snap/common/Zotero/storage/Y5Z59BSN/2022-23266-001.html:text/html},
