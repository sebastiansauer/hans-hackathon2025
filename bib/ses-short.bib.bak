	editor = {Camarinha-Matos, Luis M. and Ortiz, Angel and Boucher, Xavier and Osório, A. Luís},
	date = {2022},
	langid = {english},
	keywords = {Social robots, Robots, {HRI}, Scale construction, Social perception, {SPRS}-social perception of robots scale},
}

@inproceedings{ninomiya_development_2015,
