@misc{lehmann_how_2023,
	location = {Rochester, {NY}},
	title = {How Do Heavy Tailed Distributions Affect the Analysis of Psychometric Bipolar Scales Data?},
	url = {https://papers.ssrn.com/abstract=4412962},
	doi = {10.2139/ssrn.4412962},
	abstract = {Psychometric data of bipolar scales are commonly used in medical and economic psychology. Recently, their compositional structure (the Simplex) was revealed. Disregarding the Simplex causes serious bias, e.g., while estimating correlations or psychometric standards. The isometric log-ratio (ilr) transformation converts Simplex data towards the interval scale and provides unbiased statistical results. The ilr approach increases the statistical power of the well-known correlation test based on Student’s t-distribution if the prerequisites of the central limit theorem ({CLT}) are fulfilled. Concerning ilr transformed data, the {CLT} is applicable if and only if the number of summands is sufficiently large and the data generating process ({DGP}) provides finite variance. In practice, however, the number of items of a questionnaire often is small and the {DGP} could be heavy-tailed. By means of a simulation study, this paper shows that the ilr approach works satisfactory if the {DGP} is heavy-tailed with finite (Laplace) or infinite (Cauchy) variance. A violation of the {CLT} hardly affects the results of the ilr approach. The results of the simulation study generalize from correlation analysis towards a larger set of standard statistical procedures, e.g. linear regression, two-sample t-test and partial least squares structural equation modeling.},
	number = {4412962},
	author = {Lehmann, René and Vogt, Bodo},
	urldate = {2023-07-20},
	date = {2023-04-12},
	langid = {english},
	keywords = {psychometrics, bipolar scale, compositional data, correlation, heavy-tailed distribution, Social Psychology, statistical power},
	file = {Lehmann&Vogt_(2023)How Do Heavy Tailed Distributions Affect the Analysis of Psychometric Bipolar Scales Data.pdf:/Users/sebastiansaueruser/Literatur/Lehmann&Vogt_(2023)How Do Heavy Tailed Distributions Affect the Analysis of Psychometric Bipolar Scales Data.pdf:application/pdf},
}

@book{lehmann_reconsidering_2023,
	title = {Reconsidering Bipolar Scales Data As Compositional Data Improves Psychometric Healthcare Data Analytics},
	isbn = {978-0-9981331-6-4},
	url = {https://hdl.handle.net/10125/102980},
	abstract = {Correct psychometric profiling and the choice of adequate therapeutic measures are the basis of any psychotherapeutic treatment. The preparation of a correct psychological profile benefits the patient and saves time and costs. Regarding psychometric questionnaires it is common practice to consider data of bipolar scales as interval scaled. This paper reveals the true compositional data structure (namely the Simplex) with respect to the psychometric limit of quantification of bipolar traits and constructs. The Simplex heavily affects the set of statistical procedures applicable. Disregarding the Simplex causes serious bias and results in erroneous standards and standard deviations, biased correlations, reduced convergent validity and a loss of statistical power. In this paper, the isometric log-ratio (ilr) transformation is suggested. It transforms Simplex data towards the interval scale and provides unbiased results, e.g., standards. By means of a simulation study, this paper shows that up to an 18{\textbackslash}\% increase in the statistical power of the well-known correlation test based on Student's t-distribution can be achieved. As the statistical power increases the sample size of psychometric studies can be reduced resulting in lower data collection costs. Besides economic and psychotherapeutic aspects, the results of the simulation study generalize from correlation analysis towards a larger set of standard statistical procedures. For example, testing the hypothesis of equality of the two means of independent samples using a t-test based on Student's t distribution is equivalent to testing the hypothesis of a null-correlation between the binary grouping variable and the dependent variable. Furthermore, the coefficient of correlation contributes to the slope of a regression line. Thus, the ilr approach also affects linear regression techniques.},
	author = {Lehmann, Rene and Vogt, Bodo},
	urldate = {2023-07-20},
	date = {2023-01-03},
	file = {Lehmann&Vogt_(2023)Reconsidering Bipolar Scales Data As Compositional Data Improves Psychometric Healthcare Data Analytics.pdf:/Users/sebastiansaueruser/Literatur/Lehmann&Vogt_(2023)Reconsidering Bipolar Scales Data As Compositional Data Improves Psychometric Healthcare Data Analytics.pdf:application/pdf},
}

@article{smithson_compositional_2022,
	title = {Compositional data analysis tutorial},
	issn = {1939-1463},
	doi = {10.1037/met0000464},
	abstract = {This article presents techniques for dealing with a form of dependency in data arising when numerical data sum to a constant for individual cases, that is, “compositional” or “ipsative” data. Examples are percentages that sum to 100, and hours in a day that sum to 24. Ipsative scales fell out of fashion in psychology during the 1960s and 1970s due to a lack of methods for analyzing them. However, ipsative scales have merits, and compositional data commonly occur in psychological research. Moreover, as we demonstrate, sometimes converting data to a compositional form yields insights not otherwise accessible. Fortunately, there are sound methods for analyzing compositional data. We seek to enable researchers to analyze compositional data by presenting appropriate techniques and illustrating their application to real data. First, we elaborate the technical details of compositional data and discuss both established and new approaches to their analysis. We then present applications of these methods to real social science data-sets (data and code using R are available in a supplementary document). We conclude with a discussion of the state of the art in compositional data analysis and remaining unsolved problems. A brief guide to available software resources is provided in the first section of the supplementary document. ({PsycInfo} Database Record (c) 2022 {APA}, all rights reserved)},
	pages = {No Pagination Specified--No Pagination Specified},
	journaltitle = {Psychological Methods},
	author = {Smithson, Michael and Broomell, Stephen B.},
	date = {2022},
	note = {Place: {US}
Publisher: American Psychological Association},
	keywords = {Mathematical Modeling, Statistical Regression, Statistical Analysis, Social Sciences, Statistical Data},
	file = {Smithson&Broomell_(2022)Compositional data analysis tutorial.pdf:/Users/sebastiansaueruser/Literatur/Smithson&Broomell_(2022)Compositional data analysis tutorial.pdf:application/pdf;Snapshot:/home/sebastian/snap/zotero-snap/common/Zotero/storage/LYX53QMJ/2022-27232-001.html:text/html},
}

