	file = {Gambari et al_(2015)Effectiveness of PowerPoint Presentation on Students' Cognitive Achievement in Technical Drawing.pdf:/Users/sebastiansaueruser/Literatur/Gambari et al_(2015)Effectiveness of PowerPoint Presentation on Students' Cognitive Achievement in Technical Drawing.pdf:application/pdf},
